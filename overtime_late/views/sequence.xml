<odoo>
    <data>

    <!--  ir.sequence  -->
    <record id="excuse_seq" model="ir.sequence">
        <field name="name">Excuse sequence</field>
        <field name="code">hr.excuse</field>
        <field name="prefix">EXCUSE/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="use_date_range"/>
        <field name="padding">4</field>
    </record>
    <!--  ir.sequence  -->
    <record id="overtime_seq" model="ir.sequence">
        <field name="name">Overtime sequence</field>
        <field name="code">overtime.assignment</field>
        <field name="prefix">OVT/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="use_date_range"/>
        <field name="padding">4</field>
    </record>

    </data>
</odoo>